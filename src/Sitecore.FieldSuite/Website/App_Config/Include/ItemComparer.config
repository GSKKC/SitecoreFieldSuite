<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<itemComparer>
			<validation type="Sitecore.SharedSource.PublishedItemComparer.Validations.ItemValidator, Sitecore.SharedSource.PublishedItemComparer" />
			<validation type="Sitecore.SharedSource.PublishedItemComparer.Validations.AncestorValidator, Sitecore.SharedSource.PublishedItemComparer" />
			<validation type="Sitecore.SharedSource.PublishedItemComparer.Validations.PresentationDetailValidator, Sitecore.SharedSource.PublishedItemComparer" />
			<validation type="Sitecore.SharedSource.PublishedItemComparer.Validations.ItemsTemplateValidator, Sitecore.SharedSource.PublishedItemComparer" />
			<validation type="Sitecore.SharedSource.PublishedItemComparer.Validations.TemplateValidator, Sitecore.SharedSource.PublishedItemComparer" />
		</itemComparer>
		<commands>
			<command name="velir:ItemComparerViewer" type="Sitecore.SharedSource.PublishedItemComparer.CustomSitecore.Commands.ItemComparerCommand, Sitecore.SharedSource.PublishedItemComparer" />
			<command name="velir:TargetDatabase" type="Sitecore.SharedSource.PublishedItemComparer.CustomSitecore.Commands.TargetDatabaseCommand, Sitecore.SharedSource.PublishedItemComparer" />
		</commands>
	</sitecore>
</configuration>