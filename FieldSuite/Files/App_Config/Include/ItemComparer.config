<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<itemComparer>
			<validation type="Velir.SitecoreLibrary.Modules.PublishedItemComparer.Validations.ItemValidator, Velir.SitecoreLibrary.Modules.PublishedItemComparer" />
			<validation type="Velir.SitecoreLibrary.Modules.PublishedItemComparer.Validations.AncestorValidator, Velir.SitecoreLibrary.Modules.PublishedItemComparer" />
			<validation type="Velir.SitecoreLibrary.Modules.PublishedItemComparer.Validations.PresentationDetailValidator, Velir.SitecoreLibrary.Modules.PublishedItemComparer" />
			<validation type="Velir.SitecoreLibrary.Modules.PublishedItemComparer.Validations.ItemsTemplateValidator, Velir.SitecoreLibrary.Modules.PublishedItemComparer" />
			<validation type="Velir.SitecoreLibrary.Modules.PublishedItemComparer.Validations.TemplateValidator, Velir.SitecoreLibrary.Modules.PublishedItemComparer" />
		</itemComparer>
		<commands>
			<command name="velir:ItemComparerViewer" type="Velir.SitecoreLibrary.Modules.PublishedItemComparer.CustomSitecore.Commands.ItemComparerCommand, Velir.SitecoreLibrary.Modules.PublishedItemComparer" />
			<command name="velir:TargetDatabase" type="Velir.SitecoreLibrary.Modules.PublishedItemComparer.CustomSitecore.Commands.TargetDatabaseCommand, Velir.SitecoreLibrary.Modules.PublishedItemComparer" />
		</commands>
	</sitecore>
